cols <- c("low" = "red","normal" = "blue","high" = "orange")
blank_theme <- theme_minimal()+
theme(
axis.text = element_blank(),
plot.title=element_text(size=20, face="bold"),
legend.text = element_text(size=14, face="bold"),
legend.title = element_text(size=16, face="bold"),
axis.title = element_text(size=16),
legend.background=element_rect(fill = "transparent",colour='lightblue')
)
p_boxplot = ggplot(data = white_wine, aes(x = ordered_category, y = alcohol)) +
geom_boxplot(aes(fill= ordered_category)) +
xlab('') +
ylab('Alcohol (% by volume)') +
scale_fill_manual('Categories of Wine Quality', values = cols, breaks = c("low", "normal", "high"), labels = c("Low Quality (Grade 1, 2, 3 and 4)", "Normal Quality (Grade 5, 6 and 7)", "High Quality(Grade 8 and 9)")) +
labs(title = "Alcohol By Wine Qualities") +
blank_theme
p_density = ggplot(data = white_wine, aes(alcohol, ..density..)) +
geom_density(aes(colour = ordered_category)) +
xlab('Alcohol (% by volume)') +
ylab('Density') +
scale_colour_manual('Categories of Wine Quality', values = cols, breaks = c("low", "normal", "high"), labels = c("Low Quality (Grade 1, 2, 3 and 4)", "Normal Quality (Grade 5, 6 and 7)", "High Quality(Grade 8 and 9)")) +
labs(title = "Density of Alcohol By Wine Qualities") +
blank_theme
grid.arrange(p_boxplot,p_density, nrow=2)
